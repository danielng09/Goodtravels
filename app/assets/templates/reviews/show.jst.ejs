<div class='avatar-thumb' style=
  '<%= 'background-image: url(' + user.get('image_url') + ');' %>'>
</div>

<div class="review-details col-md-9">
  <p class='review-username'><%= user.get('username') %></p>
  <p class='review-date'><%= review.get('created_at') %></p>
  <div class='review-star-rating'></div>
  <p class='review-body'><%= review.escape('body') %></p>
  <% if (review.get('users_review')) { %>
    <button type="button" class="edit-review btn btn-sm btn-success">
      <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
    </button>
  <% } %>
</div>
